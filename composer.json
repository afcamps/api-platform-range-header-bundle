{
  "name": "campingcom/api-platform-range-header-bundle",
  "type": "symfony-bundle",
  "description": "API Platform range header pagination Bundle",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "api-platform/core": "^2.7|^3.0"
  },
  "require-dev": {
    "behat/behat": "^3.11",
    "behat/mink": "^1.10",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.13",
    "fakerphp/faker": "^1.20",
    "friends-of-behat/mink-browserkit-driver": "^1.6",
    "friends-of-behat/mink-extension": "^2.7",
    "friends-of-behat/symfony-extension": "^2.3",
    "friendsofphp/php-cs-fixer": "^3.13",
    "nelmio/cors-bundle": "^2.2",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpmd/phpmd": "^2.13",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpdoc-parser": "^1.13",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpstan/phpstan-symfony": "^1.2",
    "phpstan/phpstan-webmozart-assert": "^1.2",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "6.0.*|6.1.*",
    "symfony/console": "6.0.*|6.1.*",
    "symfony/dotenv": "6.0.*|6.1.*",
    "symfony/expression-language": "6.0.*|6.1.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.0.*|6.1.*",
    "symfony/property-access": "6.0.*|6.1.*",
    "symfony/property-info": "6.0.*|6.1.*",
    "symfony/proxy-manager-bridge": "6.0.*|6.1.*",
    "symfony/runtime": "6.0.*|^6.1",
    "symfony/security-bundle": "6.0.*|6.1.*",
    "symfony/serializer": "6.0.*|6.1.*",
    "symfony/twig-bundle": "6.0.*|6.1.*",
    "symfony/validator": "6.0.*|6.1.*",
    "symfony/yaml": "6.0.*|6.1.*"
  },
  "autoload": {
    "psr-4": {
      "Campings\\Bundle\\ApiPlatformRangeHeaderBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Campings\\Bundle\\ApiPlatformRangeHeaderBundle\\Tests\\": "tests/",
      "Campings\\Bundle\\ApiPlatformRangeHeaderBundle\\Tests\\Application\\": "tests/Application/src/"
    },
    "classmap": [
      "tests/Application/src/Kernel.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "symfony/flex": false,
      "symfony/runtime": true
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
